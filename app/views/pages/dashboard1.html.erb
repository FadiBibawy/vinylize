<div class="container row">

  <div class="uploaded col-lg-8 col-md-6 col-sm-6">
    <h4>My Uploaded Vinyls</h4>

    <div class="row d-flex justify-content-around">
      <% @vinyls.each do |vinyl|%>
        <%# <div class="card text-white card-has-bg click-col" style="background-image:url('https://source.unsplash.com/600x900/?tech,street');"> %>

        <%# <div class="card col-lg-4 col-md-6 p-1 mb-5"> %>
          <% if vinyl.photo.key %>
            <div class="card col-lg-4 col-md-6 text-white card-has-bg click-col" style="background-image: url(<%= rails_blob_url(vinyl.photo) %>);">
          <% end %>
          <div class="card-body">
            <h4 class="card-title"><%= link_to vinyl.record_title, vinyl_path(vinyl)%></h4>
            <p class="card-text">Card text</p>
            <%= link_to "See Vinyl", vinyl_path(vinyl), class:"btn btn-primary"%>
          </div>
        </div>
      <% end %>
    </div>

  </div>


  <div class="bookings col-lg-4 col-md-6 col-sm-6 border-start">
    <h4>My Bookings ðŸ§¾</h4>
    <ul class="list-group">

      <% @bookings.each do |booking| %>
        <li class="list-group-item active">Vinyl title: <strong class="text-light bg-dark"><%= link_to booking.vinyl.record_title, vinyl_path(booking.vinyl)%></strong> </li>
        <li class="list-group-item">start date: <%= booking.start_date %></li>
        <li class="list-group-item">end date: <%= booking.end_time %></li>
        <li class="list-group-item">total price: <%= booking.total_price %>â‚¬</li>
        <li class="list-group-item"><%= button_to("Delete", booking_path(booking), method: :delete, class: 'btn btn-danger') %></li>
      <% end %>
    </ul>

  </div>
</div>



